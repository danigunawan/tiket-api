{
    "name": "itsmeabde/tiket-api",
    "type": "library",
    "description": "Laravel package for using api tiket.com",
    "keywords": [
        "itsmeabde",
        "tiket api"
    ],
    "homepage": "https://github.com/itsmeabde/tiket-api",
    "license": "MIT",
    "authors": [
        {
            "name": "itsmeabde",
            "email": "itsmeabde@gmail.com",
            "homepage": "https://github.com/itsmeabde",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "itsmeabde\\TiketApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "itsmeabde\\TiketApi\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
